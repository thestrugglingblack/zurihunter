                                            <div class="col-3 icon-block"><i class="fa fa-laptop"></i></div>
                                            <div class="flot-left">
                                                <h5>{{universityName}}</h5>
                                                <h4>{{major}}</h4>
                                                <span>{{degreeType}}</span>
                                            </div>
                                        </div>
                                        <div class="col-6 margin-bottom50 margin-top50 no-margin-top">
                                            {{description}}
                                        </div>
                                    </div>
                                    <div class="full-divider"></div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </article>

            <!-- Publications & Achievements -->
            <article class="content publicationsAchievements white-bg" id="chapterpublicationsachievements">
                <div class="inner">
                    <h2>Publications & Achievements</h2>
                    <div class="title-divider"></div>
                    <div class="education-con">
                        <div class="container-sub skill-list">
