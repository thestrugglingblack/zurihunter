                                        <div class="col-6 margin-bottom50 margin-top50">
                                            <div class="col-3 icon-block"><i class="{{icon}}"></i></div>
                                            <div class="flot-left">
                                                <h5>{{companyName}}</h5>
                                                <h4>{{position}}</h4>
                                                <span>{{years}}</span></div>
                                        </div>
                                        <div class="col-6 margin-bottom50 margin-top50 no-margin-top">
                                            <ul>
                                                {{#each description}}
                                                    <li>{{this}}</li>
                                                {{/each}}
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="full-divider"></div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </article>

            <!-- Education -->
            <article class="content education gray-bg" id="chaptereducation">
