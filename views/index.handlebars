            <!-- Skills -->
            <article class="content skills gray-bg" id="chapterskills">
                <div class="inner">
                    <h2>Skills</h2>
                    <div class="title-divider"></div>
                    <h3>Jack of All Trades</h3>
                    <p>{{content.skillsSubHeader}}</p>
                    <div class="skills-con">
                        <div class="container-sub margin-top50">
                            <div class="row">
                                <div class="col-6 margin-bottom50">
                                    <div class="col-6"><span class="chart" data-percent="85"><span
                                            class="percent"></span> </span></div>
                                    <div class="col-6 chart-text">
                                        <h4>Back-End Development</h4>
                                        <p>Proficient, {{content.yearsOfExp}} years</p>
                                    </div>
                                </div>
                                <div class="col-6 margin-bottom50">
                                    <div class="col-6"><span class="chart" data-percent="75"><span
                                            class="percent"></span> </span></div>
                                    <div class="col-6 chart-text">
                                        <h4>Front-End Development</h4>
                                        <p>Proficient, {{content.yearsOfExp}} years</p>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="col-6"><span class="chart" data-percent="80"><span
                                            class="percent"></span> </span></div>
                                    <div class="col-6 chart-text">
                                        <h4>Data Engineering</h4>
                                        <p>Competent, 2 years</p>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="col-6"><span class="chart" data-percent="65"><span
                                            class="percent"></span> </span></div>
                                    <div class="col-6 chart-text">
                                        <h4>Scrum Master</h4>
                                        <p>Novice, 1 years</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="full-divider"></div>
                        <div class="container-sub skill-list">
                            <div class="row">
                                <h3>Technologies</h3>
                                <p>{{content.technologiesSubHeader}}</p>
                                <div class="col-4 margin-top10">
                                    <ul>
                                        <b>Databases</b>
                                        {{#each content.databases}}
                                            <li>{{this}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                                <div class="col-4 margin-top10">
                                    <ul>
                                        <b>Cloud Technologies</b>
                                        {{#each content.cloudTechnologies}}
                                            <li>{{this}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                                <div class="col-4 margin-top10">
                                    <ul>
                                        <b>Tooling and Libraries</b>
                                        {{#each content.toolingLibraries}}
                                            <li>{{this}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="full-divider"></div>
                        <div class="container-sub">
                            <div class="row">
                                <h3>Programming Languages</h3>
                                <p>{{content.programmingLanguagesSubHeader}}</p>
                                {{#each content.programmingLanguage}}
