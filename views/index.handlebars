            <!-- Contact -->
            <article class="content contact gray-bg" id="chaptercontact">
                <div class="inner">
                    <h2>Contact</h2>
                    <div class="title-divider"></div>
                    <h3>Let's Keep In Touch</h3>
                    <p>{{content.contactSubHeader}}</p>

                    <div class="contact-con margin-top50">
                        <div class="container-sub">
                            <div class="row">
                                <div class="contact-details">
                                    <div class="col-12">
                                        <div class="contact-text">
                                            <div class="col-2 icon-block address"><i class="fa fa-twitter"></i></div>
                                            <div class="flot-left"><a href={{content.socialMedia.twitter}}> Twitter<br/> <strong>@ZuriHunter</strong></a><br>
                                            </div>
                                        </div>
                                        <div class="contact-text">
                                            <div class="col-2 icon-block phone"><i class="fa fa-linkedin"></i></div>
                                            <div class="flot-left"><a href={{content.socialMedia.linkedin}}> LinkedIn<br/> <strong>Zuri Hunter</strong></a><br>
                                            </div>
                                        </div>
                                        <div class="contact-text">
                                            <div class="col-2 icon-block email"><i class="fa fa-twitch"></i></div>
                                            <div class="flot-left"><a href={{content.socialMedia.twitch}}> Twitch<br/> <strong>@thestrugglingblack</strong></a><br>
                                            </div>
                                        </div>
                                        <div class="contact-text">
                                            <div class="col-2 icon-block email"><i class="fa fa-gitlab"></i></div>
                                            <div class="flot-left"><a href={{content.socialMedia.gitlab}}> GitLab<br/> <strong>@thestrugglingblack</strong></a><br>
                                            </div>
                                        </div>
                                        <div class="contact-text">
                                            <div class="col-2 icon-block email"><i class="fa fa-github"></i></div>
                                            <div class="flot-left"><a href={{content.socialMedia.github}}> GitHub<br/> <strong>@thestrugglingblack</strong></a><br>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="full-divider"></div>
                        <div class="container-sub">
                            <div class="row">
                                <div class="contact-form">
                                    <h3>Drop Me a Line</h3>
                                    <form id="form1" name="form1" method="post">
                                        <input name="name" type="text" id="name" placeholder="Your Name..."/>
                                        <input name="email" type="text" id="email" placeholder="Your Email..."/>
